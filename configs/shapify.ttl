@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix sosa: <http://www.w3.org/ns/sosa/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix js: <https://w3id.org/conn/js#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <https://w3id.org/conn#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix dc: <http://purl.org/dc/terms/>.
@prefix rdfl: <https://w3id.org/rdf-lens/ontology#>.
@prefix rdfc: <https://w3id.org/rdf-connect/ontology#>.

rdfc:Shapify a owl:Class, rdfs:Class;
  rdfc:jsImplementationOf rdfc:Processor;
  rdfs:label "Shapify processor";
  dc:title "Shapify processor";
  dc:description "";
  rdfc:file <../lib/shapify.js>;
  rdfc:class "Shapify";
  rdfc:entrypoint <../>.

[ ] a sh:NodeShape;
  sh:targetClass <Shape>;
  sh:property [
    sh:datatype xsd:any;
    sh:path ( );
    sh:name "id";
    sh:minCount 1;
    sh:maxCount 1;
  ], [
    sh:class rdfl:Context;
    sh:path ( );
    sh:name "quads";
    sh:minCount 1;
    sh:maxCount 1;
  ].

[ ] a sh:NodeShape;
  sh:targetClass rdfc:Shapify;
  sh:property [
    sh:class rdfc:Reader;
    sh:path js:input;
    sh:name "reader";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:class rdfc:Writer;
    sh:path js:output;
    sh:name "writer";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:class <Shape>;
    sh:path js:shape;
    sh:name "shape";
    sh:minCount 1;
    sh:maxCount 1;
  ].

