@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix js: <https://w3id.org/conn/js#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <https://w3id.org/conn#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix dc: <http://purl.org/dc/terms/>.
@prefix rdfc: <https://w3id.org/rdf-connect/ontology#>.

rdfc:Ldesify a owl:Class, rdfs:Class;
  rdfc:jsImplementationOf rdfc:Processor;
  rdfs:label "Ldesify processor";
  dc:title "Ldesify processor";
  rdfc:file <../lib/ldesify.js>;
  rdfc:class "Ldesify";
  rdfc:entrypoint <../>.

[ ] a sh:NodeShape;
  sh:targetClass rdfc:Ldesify;
  sh:property [
    sh:class rdfc:Reader;
    sh:path js:input;
    sh:name "reader";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:class rdfc:Writer;
    sh:path js:output;
    sh:name "writer";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:datatype xsd:boolean;
    sh:path js:checkProps;
    sh:name "check_properties";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:iri;
    sh:path js:timestampPath;
    sh:name "modifiedPath";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:iri;
    sh:path js:versionOfPath;
    sh:name "isVersionOfPath";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path js:path;
    sh:name "statePath";
    sh:maxCount 1;
  ].

rdfc:LdesifySDS a owl:Class, rdfs:Class;
  rdfc:jsImplementationOf rdfc:Processor;
  dc:title "Ldesify processor (for sds streams)";
  rdfs:label "Ldesify processor (for sds streams)";
  dc:description "";
  rdfc:file <../lib/ldesify.js>;
  rdfc:class "LdesifySDS";
  rdfc:entrypoint <../>.

[ ] a sh:NodeShape;
  sh:targetClass rdfc:LdesifySDS;
  sh:property [
    sh:class rdfc:Reader;
    sh:path js:input;
    sh:name "reader";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:class rdfc:Writer;
    sh:path js:output;
    sh:name "writer";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:datatype xsd:string;
    sh:path js:statePath;
    sh:name "statePath";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:iri;
    sh:path js:sourceStream;
    sh:name "sourceStream";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:iri;
    sh:path js:targetStream;
    sh:name "targetStream";
    sh:minCount 1;
    sh:maxCount 1;
  ], [
    sh:datatype xsd:iri;
    sh:path js:timestampPath;
    sh:name "modifiedPathM";
    sh:maxCount 1;
  ], [
    sh:datatype xsd:iri;
    sh:path js:versionOfPath;
    sh:name "isVersionOfPathM";
    sh:maxCount 1;
  ].

