@prefix js: <https://w3id.org/conn/js#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix fnom: <https://w3id.org/function/vocabulary/mapping#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix : <https://w3id.org/conn#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.

rdfc:LdesDiskWriter a rdfc:Processor;
  rdfc:file <../lib/ldesDiskWriter.js>;
  rdfc:class "LdesDiskWriter";
  rdfc:entrypoint <../>.

[ ] a sh:NodeShape;
  sh:targetClass rdfc:LdesDiskWriter;
  sh:property [
    sh:class rdfc:Reader;
    sh:path js:dataInput;
    sh:name "Data Input Channel";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:class rdfc:Reader;
    sh:path js:metadataInput;
    sh:name "Metadata Input Channel";
    sh:maxCount 1;
    sh:minCount 1;
  ], [
    sh:datatype  xsd:string;
    sh:path js:directory;
    sh:name "Directory";
    sh:maxCount 1;
    sh:minCount 1;
  ].
